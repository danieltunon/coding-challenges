{
  "env": {
      "browser": true,
      "es6": true,
      "node": true
  },
  "extends": "airbnb",
  "rules": {
      "indent": [
          2,
          2
      ],
      "linebreak-style": [
          2,
          "unix"
      ],
      "quotes": [
          2,
          "single"
      ],
      "semi": [
          2,
          "always"
      ],
      "no-console": [
        0
      ],
      "spaced-comment": [
        0
      ]
  },
  "globals": {
    "_": true,
    "describe": true,
    "expect": true,
    "it": true,
    "beforeEach": true,
    "sinon": true,
    "clock": true
  }
}
